{
    "api_url": {
        "type": "string",
        "description": "XHS-Downloader 解析服务的 API 地址",
        "default": "http://127.0.0.1:5556/xhs/"
    },
    "douyin_cookie": {
        "type": "string",
        "description": "抖音Cookie (s_v_web_id)。",
        "default": ""
    },
    "enable_download_cache": {
        "type": "bool",
        "description": "是否启用下载缓存。",
        "default": true
    },
    "show_all_progress_tips": {
        "type": "bool",
        "description": "是否显示详细进度提示。",
        "default": false
    },
    "cache_dir": {
        "type": "string",
        "description": "自定义缓存目录路径。",
        "default": ""
    },
    "cache_cleanup_interval": {
        "type": "int",
        "description": "缓存自动清理间隔（秒）。",
        "default": 3600
    },
    "bili_use_login": {
        "type": "bool",
        "description": "是否使用登录状态解析B站 (影响画质)。",
        "default": false
    },
    "bili_download_video": {
        "type": "bool",
        "description": "B站解析是否下载并发送视频文件。关闭(False)则只发送封面和直链。默认关闭。",
        "default": false
    }
}